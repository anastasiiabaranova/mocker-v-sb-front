{
	"name": "mocker-e2e",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/mocker-e2e/src",
	"projectType": "application",
	"targets": {
		"e2e": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"devServerTarget": "mocker:serve",
				"watch": true,
				"headless": false,
				"cypressConfig": "apps/mocker-e2e/cypress.config.ts"
			},
			"configurations": {
				"production": {
					"devServerTarget": "mocker:serve:production"
				}
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/mocker-e2e/**/*.{js,ts}"]
			}
		},
		"cypress-run": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"devServerTarget": "mocker:serve",
				"cypressConfig": "apps/mocker-e2e/cypress.config.ts"
			},
			"configurations": {
				"production": {
					"devServerTarget": "mocker:serve:production"
				}
			}
		},
		"cypress-open": {
			"executor": "@nrwl/cypress:cypress",
			"options": {
				"watch": true,
				"headless": false,
				"cypressConfig": "apps/mocker-e2e/cypress.config.ts"
			}
		}
	},
	"tags": [],
	"implicitDependencies": ["mocker"]
}
