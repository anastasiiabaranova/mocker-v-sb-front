{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": ".",
		"outDir": "./dist/out-tsc",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitOverride": true,
		"noPropertyAccessFromIndexSignature": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"sourceMap": true,
		"declaration": false,
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"target": "ES2022",
		"module": "ES2022",
		"useDefineForClassFields": false,
		"lib": ["ES2022", "dom"],
		"esModuleInterop": true,
		"paths": {
			"@mocker/auth/api": ["libs/auth/api/src/index.ts"],
			"@mocker/auth/domain": ["libs/auth/domain/src/index.ts"],
			"@mocker/auth/feature-auth": [
				"libs/auth/feature-auth/src/index.ts"
			],
			"@mocker/auth/shell": ["libs/auth/shell/src/index.ts"],
			"@mocker/graphql/api": ["libs/graphql/api/src/index.ts"],
			"@mocker/graphql/domain": ["libs/graphql/domain/src/index.ts"],
			"@mocker/graphql/feature-graphql": [
				"libs/graphql/feature-graphql/src/index.ts"
			],
			"@mocker/graphql/feature-graphql-service": [
				"libs/graphql/feature-graphql-service/src/index.ts"
			],
			"@mocker/graphql/feature-graphql-service-list": [
				"libs/graphql/feature-graphql-service-list/src/index.ts"
			],
			"@mocker/graphql/history/domain": [
				"libs/graphql/history/domain/src/index.ts"
			],
			"@mocker/graphql/history/feature-graphql-history": [
				"libs/graphql/history/feature-graphql-history/src/index.ts"
			],
			"@mocker/graphql/history/shell": [
				"libs/graphql/history/shell/src/index.ts"
			],
			"@mocker/graphql/shell": ["libs/graphql/shell/src/index.ts"],
			"@mocker/mq/domain": ["libs/mq/domain/src/index.ts"],
			"@mocker/mq/feature-mq": ["libs/mq/feature-mq/src/index.ts"],
			"@mocker/mq/feature-mq-topic": [
				"libs/mq/feature-mq-topic/src/index.ts"
			],
			"@mocker/mq/feature-mq-topic-list": [
				"libs/mq/feature-mq-topic-list/src/index.ts"
			],
			"@mocker/mq/shell": ["libs/mq/shell/src/index.ts"],
			"@mocker/rest/domain": ["libs/rest/domain/src/index.ts"],
			"@mocker/rest/feature-rest": [
				"libs/rest/feature-rest/src/index.ts"
			],
			"@mocker/rest/feature-rest-service": [
				"libs/rest/feature-rest-service/src/index.ts"
			],
			"@mocker/rest/feature-rest-service-list": [
				"libs/rest/feature-rest-service-list/src/index.ts"
			],
			"@mocker/rest/shell": ["libs/rest/shell/src/index.ts"],
			"@mocker/shared/ui": ["libs/shared/ui/src/index.ts"],
			"@mocker/shared/utils": ["libs/shared/utils/src/index.ts"],
			"@mocker/rest/history/domain": ["libs/rest/history/domain/src/index.ts"],
			"@mocker/rest/history/feature-rest-history": [
				"libs/rest/history/feature-rest-history/src/index.ts"
			],
			"@mocker/rest/history/shell": ["libs/rest/history/shell/src/index.ts"]
		},
		"types": ["node"],
		"rootDir": "."
	},
	"angularCompilerOptions": {
		"enableI18nLegacyMessageIdFormat": false,
		"strictInjectionParameters": true,
		"strictInputAccessModifiers": true,
		"strictTemplates": true
	},
	"exclude": ["node_modules", "tmp"]
}
